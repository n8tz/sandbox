{
  "www": {
    "basedOn": "defaultBrowser",
    "rootFolder": "App",
    "libsPath": "./layers",
    "vars": {
      "targetDir": "dist/www",
      "extractCss": true,
      "production": true,
      "DefinePluginCfg": {
        "__IS_DEV__": false,
        "__IS_SERVER__": false
      },
      "webpackPatch": {
        "mode": "production"
      }
    },
    "extend": [
      "widget-management",
      "wi-layer-redux-thunk"
    ]
  },
  "api": {
    "basedOn": "defaultServer",
    "rootFolder": "App",
    "libsPath": "./layers",
    "vars": {
      "targetDir": "dist/api",
      "DefinePluginCfg": {
        "__IS_DEV__": false,
        "__IS_SERVER__": true
      }
    },
    "extend": [
      "widget-management",
      "wi-layer-redux-thunk"
    ]
  },
  "dev_www": {
    "basedOn": "defaultBrowser",
    "rootFolder": "App",
    "libsPath": "./layers",
    "vars": {
      "targetDir": "dist/www",
      "DefinePluginCfg": {
        "__IS_DEV__": true,
        "__IS_SERVER__": false
      },
      "webpackPatch": {
      }
    },
    "extend": [
      "widget-management",
      "wi-layer-redux-thunk"
    ]
  },
  "dev_api": {
    "basedOn": "defaultServer",
    "rootFolder": "App",
    "libsPath": "./layers",
    "vars": {
      "targetDir": "dist/api",
      "DefinePluginCfg": {
        "__IS_DEV__": true,
        "__IS_SERVER__": true
      },
      "externals": true
    },
    "extend": [
      "widget-management",
      "wi-layer-redux-thunk"
    ]
  }
}